{
  "name": "unicloudvn/klbpay-php-sdk",
  "type": "library",
  "description": "Client library for Kien Long bank pay",
  "homepage": "https://kienlongbank.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Unicloud Group developers",
      "homepage": "https://www.unicloud.com.vn/en/"
    }
  ],
  "keywords": [
    "KienLongPay",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "autoload": {
    "psr-4": {
